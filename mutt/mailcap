text/plain; $EDITOR %s ;
text/html; /usr/bin/brave %s %s && sleep 1.5; description=HTML Text; nametemplate=%s.html
text/html; /usr/bin/w3m -dump -T text/html -I %{charset} -O utf-8 %s; copiousoutput; description=HTML Text; nametemplate=%s.html
image/*; sxiv '%s'; test=test -n "$DISPLAY"
video/*; setsid mpv --quiet %s &; copiousoutput
audio/*; mpv %s ;
application/pdf; zathura %s ; test=test -n "$DISPLAY"
application/pgp-encrypted; gpg -d '%s'; copiousoutput;
application/pgp-keys; gpg --import '%s'; copiousoutput;
